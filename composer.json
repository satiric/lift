{
    "name": "decadal/lift",
    "description": "Lift package",
    "keywords": ["decadal"],
    "homepage": "https://toster.ru/user/Decadal",
    "license": "Proprietary",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Maruhnich Konstantin",
            "email": "nocturneumbra@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-mbstring" : "*",
        "zendframework/zend-validator": "^2.12",
        "zfcampus/zf-console": "^1.4",
        "zendframework/zend-servicemanager": "^3.4",
        "zendframework/zend-mvc": "^3.1",
        "zfcampus/zf-development-mode": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Decadal\\Lift\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Decadal\\LiftTest\\": "test"
        }
    },
    "scripts": {
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "test": "phpunit"
    }
}
